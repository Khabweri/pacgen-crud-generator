{
    "name": "pacgen/crud-generator",
    "description": "Generate CRUD operations for Laravel models with views, controllers, and routes.",
    "type": "library",
    "license": "MIT",
    "version": "6.0.0",  
    "autoload": {
        "psr-4": {
            "Pacgen\\CrudGenerator\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Andrew Khabweri",
            "email": "andrewkhabweri@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0 || ^8.2 || ^8.3",
        "laravel/framework": "^8.0 || ^9.0 || ^10.0 || ^11.0",
        "illuminate/support": "^8.0 | ^9.0 | ^10.0 | ^11.0",
        "illuminate/http": "^8.0 | ^9.0 | ^10.0 | ^11.0"
    },
    "extra": {
        "laravel": {
          "providers": [
            "Pacgen\\CrudGenerator\\Providers\\CrudGeneratorServiceProvider"
          ],
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0 | ^10.0 | ^11.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump"
        ]
    }
}
